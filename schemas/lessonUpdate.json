            "examples": [
                2
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Solfege"
            ]
        },
        "number": {
            "$id": "#/properties/number",
            "type": "integer",
            "title": "The number schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "sheet": {
            "$id": "#/properties/sheet",
            "type": "string",
            "title": "The sheet schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "www.google.com"
            ]
        },
        "sound": {
            "$id": "#/properties/sound",
            "type": "string",
            "title": "The sound schema",
