{
    "$id": "http://example.com/example.json",
    "$schema": "http://json-schema.org/lessonUpdate/schema",
    "default": {},
    "description": "The root schema comprises the entire JSON document.",
    "examples": [
        {
            "courseId": 2,
            "name": "Solfege",
            "number": 1,
            "sheet": "www.google.com",
            "sound": "www.youtube.com"
        }
    ],
    "required": [
        "courseId",
        "number"
    ],
    "title": "The root schema",
    "type": "object",
    "properties": {
        "courseId": {
            "$id": "#/properties/courseId",
            "type": "integer",
            "title": "The courseId schema",
